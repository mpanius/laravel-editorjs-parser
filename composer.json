{
	"name": "ixbtcom/laravel-editorjs-parser",
	"description": "A editor.js html parser for Laravel",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Mikhail Panyushkin",
			"email": "mpanius@gmail.com"
		},
		{
			"name": "Al-Amin Firdows",
			"email": "alaminfirdows@gmail.com"
		}
	],
	"require": {
		"php": "^8.2|^8.3|^8.4",
		"codex-team/editor.js": "^v2.0.7",
		"illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
      "ext-json": "*"
    },
	"require-dev": {
		"orchestra/testbench": "^6.0",
		"phpunit/phpunit": "^9.5"
	},
	"autoload": {
		"psr-4": {
			"Ixbtcom\\LaravelEditorJsParser\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Ixbtcom\\LaravelEditorJsParser\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Ixbtcom\\LaravelEditorJsParser\\LaravelEditorJsParserServiceProvider"
			]
		}
	}
}
